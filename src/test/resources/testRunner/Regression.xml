<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestNG Regression Testing" verbose="3" parallel="tests" thread-count="3">


<listeners>

      <listener class-name="com.aventstack.chaintest.plugins.ChainTestListener"/>
      
</listeners>

	
	
	<test name="LoginPage Test_chrome">
	<parameter name="browser" value="chrome"> </parameter>
		<classes>
			<class name="com.qa.opencart.test.LoginpageTest" />
		</classes>
	</test>
	
	
	
	
		<test name="Homepage Test">
		<parameter name="browser" value="chrome"> </parameter>
	
		<classes>
			<class name="com.qa.opencart.test.HomePageTest" />
		</classes>
	</test>
	
	
	<test name="productingfo test">
	
		<classes>
			<class name="com.qa.opencart.test.ProductinfoproductTest" />
		</classes>
	</test>
	
</suite>